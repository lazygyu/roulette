    "deleteOutDir": true,
    "assets": [
      {
        "include": "../../node_modules/box2d-wasm/dist/umd/*.wasm",
        "outDir": "./dist/assets",
        "watchAssets": true
      }
    ]
  },
  "generateOptions": {
    "spec": false
  }
}
